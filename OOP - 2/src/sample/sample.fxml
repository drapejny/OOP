<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <center>
      <Canvas fx:id="canvas" height="613.0" width="960.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <HBox prefHeight="40.0" prefWidth="822.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="circleBtn" mnemonicParsing="false" prefHeight="45.0" prefWidth="60.0" text="Круг" />
            <Button fx:id="ellipseBtn" mnemonicParsing="false" prefHeight="45.0" prefWidth="60.0" text="Эллипс" />
            <Button fx:id="rectangleBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="121.0" text="Прямоугольник" />
            <Button fx:id="squareBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="101.0" text="Квадрат" />
            <Button fx:id="triangleBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="110.0" text="Треугольник" />
            <Button fx:id="lineBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="133.0" text="Линия" />
         </children>
      </HBox>
   </top>
</BorderPane>
